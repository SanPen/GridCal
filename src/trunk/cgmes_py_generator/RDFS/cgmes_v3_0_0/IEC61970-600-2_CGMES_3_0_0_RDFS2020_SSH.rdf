	<rdfs:subClassOf rdf:resource="http://iec.ch/TC57/CIM100#Switch"/>
	<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An overcurrent protective device with a circuit opening fusible part that is heated and severed by the passage of overcurrent through it. A fuse is considered a switching device because it breaks current.</rdfs:comment>
<cims:stereotype>Description</cims:stereotype>
	<cims:belongsToCategory rdf:resource="http://iec.ch/TC57/ns/CIM/SteadyStateHypothesis-EU#Package_SteadyStateHypothesisProfile"/>
