	<rdfs:domain rdf:resource="http://iec.ch/TC57/CIM100#EquivalentInjection"/>
	<cims:dataType rdf:resource="#Voltage"/>
	<cims:multiplicity rdf:resource="http://iec.ch/TC57/1999/rdf-schema-extensions-19990926#M:0..1" />
	<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The target voltage for voltage regulation. The attribute shall be a positive value.</rdfs:comment>
	<rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
	 </rdf:Description>
	 <rdf:Description rdf:about="http://iec.ch/TC57/CIM100#EquivalentInjection.p">
