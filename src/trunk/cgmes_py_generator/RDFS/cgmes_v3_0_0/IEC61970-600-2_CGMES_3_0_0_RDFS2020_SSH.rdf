	 </rdf:Description>
	 <rdf:Description rdf:about="http://iec.ch/TC57/CIM100#RegulatingControl.minAllowedTargetValue">
	<cims:stereotype rdf:resource="http://iec.ch/TC57/NonStandard/UML#attribute"/>
